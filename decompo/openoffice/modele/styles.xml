					<style:properties text:min-label-width="0.2cm" style:font-name="Linux Biolinum G"/>
				</text:list-level-style-bullet>
			</text:list-style>
			<text:footnotes-configuration style:num-format="1" text:start-value="0" text:footnotes-position="page" text:start-numbering-at="document"/>
			<text:endnotes-configuration style:num-format="i" text:start-value="0"/>
			<text:linenumbering-configuration text:number-lines="false" text:offset="0.5cm" style:num-format="1" text:number-position="left" text:increment="5"/>
		</office:styles>
		<office:automatic-styles>
			<style:page-layout style:name="Page CV">
				<style:page-layout-properties fo:page-width="21.02cm" fo:page-height="29.73cm" style:num-format="1" style:print-orientation="portrait" fo:margin-top="1.25cm" fo:margin-bottom="1.25cm" fo:margin-left="1.25cm" fo:margin-right="1.25cm" style:writing-mode="lr-tb" style:footnote-max-height="0cm"/>
				<style:header-style/>
				<style:footer-style/>
