			</style:style>
			<style:default-style style:family="paragraph">
				<style:properties style:use-window-font-color="true" style:font-name="Linux Biolinum G" fo:font-size="12pt" fo:language="en" fo:country="US" style:font-name-asian="Linux Biolinum G" style:font-size-asian="12pt" style:language-asian="none" style:country-asian="none" style:font-name-complex="Linux Biolinum G" style:font-size-complex="12pt" style:language-complex="none" style:country-complex="none" fo:hyphenate="false" fo:hyphenation-remain-char-count="2" fo:hyphenation-push-char-count="2" fo:hyphenation-ladder-count="no-limit" style:text-autospace="ideograph-alpha" style:punctuation-wrap="hanging" style:line-break="strict" style:tab-stop-distance="1.251cm" style:writing-mode="page"/>
