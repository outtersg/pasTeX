			<style:style style:name="Texte CV, dates, technos" style:family="paragraph" style:parent-style-name="Texte CV, dates" style:class="text">
				<style:properties fo:margin-left="4.4cm" fo:margin-top="0.1cm" fo:text-indent="0cm" styleauto-text-indent="false" fo:font-style="italic" style:break-inside="avoid" fo:keep-with-next="true"/>
			</style:style>
			<style:style style:name="Texte CV, dates, technos, césure" style:family="paragraph" style:parent-style-name="Texte CV, dates, technos" style:class="text">
