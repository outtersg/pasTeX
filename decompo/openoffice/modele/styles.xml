			<style:style style:name="Nom CV" style:family="paragraph" style:parent-style-name="En-tête CV" style:next-style-name="En-tête CV">
				<style:properties fo:font-size="16pt" fo:font-weight="normal"/>
			</style:style>
			<style:style style:name="OLE" style:family="graphics">
				<style:properties text:anchor-type="paragraph" svg:x="0cm" svg:y="0cm" style:wrap="none" style:vertical-pos="top" style:vertical-rel="paragraph" style:horizontal-pos="center" style:horizontal-rel="paragraph"/>
			</style:style>
